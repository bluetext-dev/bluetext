{
  "name": "polytope-highlight",
  "displayName": "Polytope Syntax Highlighting",
  "description": "Embedded syntax highlighting for Polytope files (#pt-clj, Bash, Python)",
  "version": "0.1.0",
  "publisher": "bluetext",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "grammars": [
      {
        "path": "./syntaxes/polytope.tmLanguage.json",
        "scopeName": "polytope.injection",
        "injectTo": [
          "source.yaml"
        ]
      }
    ]
  }
}
